
var GameScene = cc.Scene.extend({
	gameLayer:null,
	frontLayer:null,
    onEnter:function () {
        this._super();

        var layer = new BgLayer();
        this.addChild(layer);
        
        this.gameLayer = new GameLayer();
        this.addChild(this.gameLayer);
        
        var frontLayer = new FrontLayer();
        frontLayer.setPoint("11111");
        this.addChild(frontLayer)
       
    },
});

